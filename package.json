{
  "name": "sjot",
  "version": "1.3.17",
  "description": "Schemas for JSON Objects, or simply SJOT, offers faster JSON validation with lightweight schemas and compact validators.  SJOT schemas have the look and feel of object templates and are easy to use.",
  "main": "sjot.js",
  "scripts": {
    "preversion": "npm test",
    "version": "./build.sh && git add -A dist",
    "postversion": "git push && git push --tags",
    "test": "./test.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Genivia/SJOT.git"
  },
  "testling": {
    "files": "dev/testling.js",
    "browsers": [
      "ie/6..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ]
  },
  "keywords": [
    "JSON",
    "lightweight",
    "schema",
    "validation",
    "compact",
    "checker",
    "validator",
    "library"
  ],
  "author": "Robert van Engelen",
